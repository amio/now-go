{
  "name": "now-go",
  "version": "2.0.0",
  "description": "Personal tinyurl service.",
  "author": "Amio <amio.cn@gmail.com>",
  "repository": "amio/now-go",
  "bin": "bin/now-go",
  "main": "index.js",
  "scripts": {
